@page
@model DiagnosticModel
<div>Claims Users</div>

<table class="table table-striped">
    <thead>
        <tr>
            <td>Name</td>
            <td>Value</td>
            <td>Issuer</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model.Diagnostic.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
                <td>@claim.Issuer</td>
            </tr>
        }
    </tbody>
</table>
